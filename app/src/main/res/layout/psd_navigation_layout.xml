<?xml version="1.0" encoding="utf-8"?>
<com.ecarx.systemui.plugin.psd.PsdNavigationBarView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/psd_navigation_bar_content"
        android:layout_width="@dimen/navi_content_width"
        android:layout_height="@dimen/navi_content_height"
        android:background="@drawable/psd_nav_content_window_bg">

        <com.ecarx.systemui.plugin.utils.AlphaImageView
            android:id="@+id/psd_navigation_bar_app_pane_iv"
            android:layout_width="@dimen/nav_icon_width"
            android:layout_height="@dimen/nav_icon_height"
            android:layout_alignParentStart="true"
            android:layout_gravity="center"
            android:layout_marginTop="160dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/ic_nav_home" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/psd_navigation_bar_hvac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</com.ecarx.systemui.plugin.psd.PsdNavigationBarView>
